{
  "name": "Password Toggler Pro",
  "version": "2.3",
  "author": "Biraj Mainali",
  "manifest_version": 3,
  "description": "Password Toggler Pro, easily hide and show your passwords",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}